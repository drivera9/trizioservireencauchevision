<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context="trizio.ram.com.triziocomandroidgradlenuevo.Bodega"
    style="@style/proyectoWMS_METRO_Theme.Layout.RelativeProcesos"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">


    <GridView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/gridView6"
        android:numColumns="2"
        android:gravity="center"
        android:layout_above="@+id/gridView12"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/gridView9" />

    <TextView
        style="@style/proyectoWMS_METRO_Theme.TextView.Titulo"
        android:text="Sucursal"
        android:id="@+id/textView24"
        android:textSize="30dp"
        />

    <GridView
        style="@style/proyectoWMS_METRO_Theme.Titulo"
        android:id="@+id/gridView9"
        android:numColumns="2"
        android:layout_height="70dp"
        android:layout_below="@+id/textView24"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <GridView
        android:layout_width="wrap_content"
        android:layout_height="80dp"
        android:id="@+id/gridView12"
        android:layout_alignParentBottom="true"
        android:numColumns="2"/>
</RelativeLayout>
